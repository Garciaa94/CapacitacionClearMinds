public class TipoPrestamo {
	private static String HIPOTECARIO ="H";
	private static String QUIROGRAFARIO ="Q";
	private static String OTROS ="O";
	/**
	 * @return the hIPOTECARIO
	 */
	public static String getHIPOTECARIO() {
		return HIPOTECARIO;
	}
	/**
	 * @param hIPOTECARIO the hIPOTECARIO to set
	 */
	public static void setHIPOTECARIO(String hIPOTECARIO) {
		HIPOTECARIO = hIPOTECARIO;
	}
	

}
