package com.cmc.excepciones;

import sun.jvm.hotspot.oops.CheckedExceptionElement;

/**
 * @author Adrian Garcia
 * @mail peter.garcia@pucese.edu.ec
 * @date  15 jul. 2021
 */
public class Cuenta {
	public void depositar(double monto) throws CheckedException{
		
		if (monto<0) {
			throw new CheckedException("Monto incorrecto");
		}
	}

}
